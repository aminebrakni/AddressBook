{% extends 'base.html.twig' %}
{% block title  %} Affiche de la société  {% endblock %}

{% block body %}

    <h1>Afficher une société</h1>

    <p>Nom de la société: <b>{{ companys.nameCompagny }}</b></p>
    <p>Ville:<b>{{ companys.city }}</b></p>

    <h2>Liste des contacts</h2>
    <ul>
        {% for contact in companys.contacts %}
        <li> {{ contact.firstName }}</li>
        {% endfor %}
    </ul>
{% endblock %}

